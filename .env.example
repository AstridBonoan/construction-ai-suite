# AI Construction Suite - Live Mode Example Environment
# Copy to .env and fill with real values for personal testing

# Database (Postgres, SQLite, etc.)
DATABASE_URL=your_db_url_here

# Flask secret key (keep private)
FLASK_SECRET_KEY=your_secret_here

# Monday.com OAuth credentials
MONDAY_CLIENT_ID=your_monday_client_id_here
MONDAY_CLIENT_SECRET=your_monday_client_secret_here
MONDAY_REDIRECT_URI=https://your-frontend.example.com/monday/oauth/callback

# Set to false for live mode, true for demo/testing
# DEMO_MODE=false
